        "@radix-ui/react-dialog": "^1.0.2",
        "@radix-ui/react-select": "^1.2.0",
        "@radix-ui/react-tooltip": "^1.0.3",
        "clsx": "^1.2.1",
        "deep-equal": "^2.1.0",
        "dexie": "^3.2.2",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "webextension-polyfill": "^0.9.0"
    }
