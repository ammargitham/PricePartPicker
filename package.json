{
    "name": "price-part-picker",
    "version": "1.0.0",
    "description": "Web Extension for displaying kakaku.com prices on pcpartpicker.com",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "dev": "webpack -w --config webpack.dev.js",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006 --no-open",
        "build-storybook": "build-storybook",
        "web-ext:chrome": "web-ext -c web-ext.chrome.config.js run --target=chromium",
        "web-ext:firefox-v2": "web-ext -c web-ext.firefox.v2.config.js run",
        "web-ext:firefox-v3": "web-ext -c web-ext.firefox.v3.config.js run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ammargitham/PricePartPicker.git"
    },
    "keywords": [
        "kakaku",
        "pcpartpicker",
        "react",
        "typescript",
        "tailwind",
        "tailwindcss",
        "chrome",
        "firefox",
        "extension"
    ],
    "author": "Ammar Githam",
    "license": "GPL-3.0-only",
    "bugs": {
        "url": "https://github.com/ammargitham/PricePartPicker/issues"
    },
    "homepage": "https://github.com/ammargitham/PricePartPicker",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.4",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/builder-webpack5": "^6.5.15",
        "@storybook/manager-webpack5": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@trivago/prettier-plugin-sort-imports": "^3.4.0",
        "@types/chrome": "^0.0.124",
        "@types/deep-equal": "^1.0.1",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.8",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "@types/react-test-renderer": "^17.0.1",
        "@types/webextension-polyfill": "^0.9.0",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "autoprefixer": "^10.4.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "css-loader": "^4.3.0",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.10",
        "filemanager-webpack-plugin": "^8.0.0",
        "jest": "^26.5.3",
        "jest-css-modules": "^2.1.0",
        "mini-css-extract-plugin": "^2.7.2",
        "postcss": "^8.4.5",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.1.2",
